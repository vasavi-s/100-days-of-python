      "source": [
        "word_list = [\"aardvark\", \"baboon\", \"camel\"]\n",
        "import random\n",
        "chosen_word=random.choice(word_list)\n",
        "guess=input(\"Guess a letter: \").lower()\n",
        "\n",
        "for letter in chosen_word:\n",
        "  if letter==guess:\n",
        "    print(\"Right\")\n",
        "  else:\n",
        "    print(\"Wrong\")"
      ],
